<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from OrderDaoService WSDL service -->

  <service name="OrderDao" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://main"/>
      <parameter name="wsdlServiceElement" value="OrderDaoService"/>
      <parameter name="schemaQualified" value="http://main"/>
      <parameter name="wsdlServicePort" value="OrderDao"/>
      <parameter name="className" value="main.OrderDaoSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="OrderDao"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="insertOrder" qname="operNS:insertOrder" xmlns:operNS="http://main" returnQName="retNS:insertOrderReturn" xmlns:retNS="http://main" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:model" xmlns:pns="http://main" type="tns:Order" xmlns:tns="http://main"/>
      </operation>
      <operation name="userOrders" qname="operNS:userOrders" xmlns:operNS="http://main" returnQName="retNS:userOrdersReturn" xmlns:retNS="http://main" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://main" returnItemQName="tns:item" xmlns:tns="http://main" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://main" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="cancelOrder" qname="operNS:cancelOrder" xmlns:operNS="http://main" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://main" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="insertOrder cancelOrder userOrders"/>

      <typeMapping
        xmlns:ns="http://main"
        qname="ns:>cancelOrderResponse"
        type="java:main.CancelOrderResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://main"
        qname="ns:>userOrders"
        type="java:main.UserOrders"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://main"
        qname="ns:>userOrdersResponse"
        type="java:main.UserOrdersResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://main"
        qname="ns:Order"
        type="java:main.Order"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://main"
        qname="ns:>insertOrderResponse"
        type="java:main.InsertOrderResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://main"
        qname="ns:>cancelOrder"
        type="java:main.CancelOrder"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://main"
        qname="ns:ArrayOf_xsd_anyType"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://main"
        qname="ns:>insertOrder"
        type="java:main.InsertOrder"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://main"
        qname="ns:Product"
        type="java:main.Product"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
