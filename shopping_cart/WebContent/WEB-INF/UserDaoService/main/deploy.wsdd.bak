<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from UserDaoService WSDL service -->

  <service name="UserDao" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://main"/>
      <parameter name="wsdlServiceElement" value="UserDaoService"/>
      <parameter name="schemaQualified" value="http://main"/>
      <parameter name="wsdlServicePort" value="UserDao"/>
      <parameter name="className" value="main.UserDaoSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="UserDao"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="userLogin" qname="operNS:userLogin" xmlns:operNS="http://main" returnQName="retNS:userLoginReturn" xmlns:retNS="http://main" returnType="rtns:User" xmlns:rtns="http://main" soapAction="" >
        <parameter qname="pns:email" xmlns:pns="http://main" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://main" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="userLogin"/>

      <typeMapping
        xmlns:ns="http://main"
        qname="ns:User"
        type="java:main.User"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
