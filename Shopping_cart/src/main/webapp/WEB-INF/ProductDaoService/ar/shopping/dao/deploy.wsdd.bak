<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ProductDaoService WSDL service -->

  <service name="ProductDao" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://dao.shopping.ar"/>
      <parameter name="wsdlServiceElement" value="ProductDaoService"/>
      <parameter name="schemaQualified" value="http://dao.shopping.ar,http://modal.shopping.ar"/>
      <parameter name="wsdlServicePort" value="ProductDao"/>
      <parameter name="className" value="ar.shopping.dao.ProductDaoSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ProductDao"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getAllProducts" qname="operNS:getAllProducts" xmlns:operNS="http://dao.shopping.ar" returnQName="retNS:getAllProductsReturn" xmlns:retNS="http://dao.shopping.ar" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://dao.shopping.ar" returnItemQName="tns:item" xmlns:tns="http://dao.shopping.ar" soapAction="" >
      </operation>
      <operation name="getCartProducts" qname="operNS:getCartProducts" xmlns:operNS="http://dao.shopping.ar" returnQName="retNS:getCartProductsReturn" xmlns:retNS="http://dao.shopping.ar" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://dao.shopping.ar" returnItemQName="tns:item" xmlns:tns="http://dao.shopping.ar" soapAction="" >
        <parameter qname="pns:cartList" xmlns:pns="http://dao.shopping.ar" type="tns:ArrayOf_xsd_anyType" xmlns:tns="http://dao.shopping.ar" itemQName="itns:item" xmlns:itns="http://dao.shopping.ar"/>
      </operation>
      <operation name="getSingleProduct" qname="operNS:getSingleProduct" xmlns:operNS="http://dao.shopping.ar" returnQName="retNS:getSingleProductReturn" xmlns:retNS="http://dao.shopping.ar" returnType="rtns:Product" xmlns:rtns="http://modal.shopping.ar" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://dao.shopping.ar" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getTotalCartPrice" qname="operNS:getTotalCartPrice" xmlns:operNS="http://dao.shopping.ar" returnQName="retNS:getTotalCartPriceReturn" xmlns:retNS="http://dao.shopping.ar" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:cartList" xmlns:pns="http://dao.shopping.ar" type="tns:ArrayOf_xsd_anyType" xmlns:tns="http://dao.shopping.ar" itemQName="itns:item" xmlns:itns="http://dao.shopping.ar"/>
      </operation>
      <parameter name="allowedMethods" value="getCartProducts getAllProducts getTotalCartPrice getSingleProduct"/>

      <typeMapping
        xmlns:ns="http://dao.shopping.ar"
        qname="ns:>getCartProducts"
        type="java:ar.shopping.dao.GetCartProducts"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dao.shopping.ar"
        qname="ns:>getCartProductsResponse"
        type="java:ar.shopping.dao.GetCartProductsResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dao.shopping.ar"
        qname="ns:>getSingleProduct"
        type="java:ar.shopping.dao.GetSingleProduct"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://modal.shopping.ar"
        qname="ns:Product"
        type="java:ar.shopping.modal.Product"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://dao.shopping.ar"
        qname="ns:ArrayOf_xsd_anyType"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dao.shopping.ar"
        qname="ns:>getSingleProductResponse"
        type="java:ar.shopping.dao.GetSingleProductResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dao.shopping.ar"
        qname="ns:>getAllProducts"
        type="java:ar.shopping.dao.GetAllProducts"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dao.shopping.ar"
        qname="ns:>getTotalCartPrice"
        type="java:ar.shopping.dao.GetTotalCartPrice"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dao.shopping.ar"
        qname="ns:>getTotalCartPriceResponse"
        type="java:ar.shopping.dao.GetTotalCartPriceResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dao.shopping.ar"
        qname="ns:>getAllProductsResponse"
        type="java:ar.shopping.dao.GetAllProductsResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
